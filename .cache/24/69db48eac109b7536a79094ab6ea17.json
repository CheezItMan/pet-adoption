({\n            breeds: data.petfinder.breeds.breed\n          });\n        } else {\n          this.setState({\n            breeds: []\n          });\n        }\n      });\n    } else {\n      this.setState({ breeds: [] });\n    }\n  };\n\n  render() {\n    return (\n      <main>\n        <header>\n          <ul>\n            <li>\n              <Link to=\"/\">Adopt Me</Link>\n            </li>\n            <li>\n              <Link to=\"/search-params\">\n                <span aria-label=\"search\" role=\"img\">\n                  üîç\n                </span>\n              </Link>\n            </li>\n          </ul>\n        </header>\n        <Provider value={this.state}>\n          <Router>\n            <Results path=\"/\" />\n            <Details path=\"/details/:id\" />\n            <SearchParams path=\"/search-params\" />\n          </Router>\n        </Provider>\n      </main>\n    );\n  }\n}\nexport default App;\n"},"lineCount":null}},"hash":"806a13230c052881269833fea64f827b","cacheData":{"env":{"API_KEY":"207018fb7db4f46cd7f9e45d66520392","API_SECRET":"3e8dc4d3f11aab35f943ec289048de00"}}}